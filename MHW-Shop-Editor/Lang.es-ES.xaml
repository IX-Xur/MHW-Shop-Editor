<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:MHW_Shop_Editor"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
    
    
    <!--application-->
    <system:String x:Key="title">MHW Shop Editor</system:String>
    <system:String x:Key="file">File</system:String>
    <system:String x:Key="open">Open</system:String>
    <system:String x:Key="save">Save</system:String>
    <system:String x:Key="lang">Language</system:String>
    <system:String x:Key="default">Default Items</system:String>
    <system:String x:Key="gems">All Gems</system:String>

    <!--itemlist-->
    <x:Array Type="local:Item" x:Key="itemsList" >
        <local:Item Key="item0000" Value="----------"/>
        <local:Item Key="item0001" Value="Item 1"/>
        <local:Item Key="item0002" Value="Item 2"/>
        <local:Item Key="item0003" Value="Item 3"/>
        <local:Item Key="item0004" Value="Item 4"/>
        <local:Item Key="item0005" Value="Item 5"/>
    </x:Array>

</ResourceDictionary>